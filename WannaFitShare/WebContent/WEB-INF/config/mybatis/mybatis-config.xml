<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- NUlL 처리 -->
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<!-- package typeAliase 처리 -->
	<typeAliases>
		<typeAlias type="com.wannafitshare.vo.Customer" alias="customer"/>
		<typeAlias type="com.wannafitshare.vo.FriendList" alias="friendList"/>
		<typeAlias type="com.wannafitshare.vo.CalorieCalendar" alias="caloriecalendar"/>
		<typeAlias type="com.wannafitshare.vo.Health" alias="health"/>
		<typeAlias type="com.wannafitshare.vo.Party" alias="party"/>
		
		<typeAlias type="com.wannafitshare.vo.PhotoUpload" alias="photoUpload"/>
		<typeAlias type="com.wannafitshare.vo.Reple" alias="reple"/>
		
	</typeAliases>
	<!-- Mapper 설정 -->
	<mappers>
		<mapper resource="common/config/mybatis/mapper/customerMapper.xml" />
		<mapper resource="common/config/mybatis/mapper/FrindListMapper.xml"/>
		<mapper resource="common/config/mybatis/mapper/loginMapper.xml"/>
		<mapper resource="common/config/mybatis/mapper/caloriecalendarMapper.xml"/>
		<mapper resource="common/config/mybatis/mapper/healthMapper.xml"/>
		<mapper resource="common/config/mybatis/mapper/partyMapper.xml"/>
		<mapper resource="common/config/mybatis/mapper/PhotoUploadMapper.xml"/>
		<mapper resource="common/config/mybatis/mapper/repleMapper.xml"/>
	</mappers>
</configuration>